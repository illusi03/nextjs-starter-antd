{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Rohman Habib M <mhrohman@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon",
    "start": "node .next_server/server/index.js",
    "export": "next export",
    "build": "npm run build:server && next build",
    "build:server": "tsc -p tsconfig.server.json",
    "build-analyze": "cross-env ANALYZE=true next build",
    "check-formatting": "prettier --list-different '{src,server,shared}/**/*.{ts,tsx}'",
    "commit": "git-cz"
  },
  "dependencies": {
    "micro": "^9.3.4",
    "microrouter": "^3.1.3",
    "next": "^9.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@next/bundle-analyzer": "^9.3.0",
    "@types/jest": "^25.1.4",
    "@types/micro": "^7.3.3",
    "@types/microrouter": "^3.1.1",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sass": "^1.26.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
